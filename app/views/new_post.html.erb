<ul class="errors">
  <% @new_post.errors.full_messages.each do |message| %>
    <li><%= message %></li>
  <% end %>
</ul>

<form method="POST" action="/new_post" 
style="width: fit-content; margin: 2em auto;"
>
  <div class="form-group">
    <label for="photo_url">Photo URL</label>
    <input type="text" id="photo_url" name="photo_url" >
  </div>

  <div class="form-group">
    <button class="button" type="submit">
    Submit
    </button>
  </div>
</form>